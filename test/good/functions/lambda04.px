
func fold(Int->Int->Int f, Int a, [Int] t) Int def
    for x in t do
        a = f(a, x)
    return a

t = [1, 2, 3, 4, 5]

print fold(_+_, 0, t)
print fold(_-_, 5, t)
print fold(_*_, 1, t)
print fold(_//-_, 360, t)

print fold(_<<_, 1, t)
print fold(_>>_, 1<<16, t)
print fold(_&_, -1, t)
print fold(_|_, 8, t)
print fold(_$_, 0, t)
print fold(_+~+_, 0, t)


func fold2(Float->Int->Float f, Float a, [Int] t) Float def
    for x in t do
        a = f(a, x)
    return a

print fold2(_/_, 30.0, t)
