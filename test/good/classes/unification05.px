
class A def
    x: Char = 'A'

    func f() def
        print this.x

class B(A) def
    constructor def
        this.x = 'B'

class C(A) def
    constructor def
        this.x = 'C'

    func f() def
        print "{this.x}" * 3


func get<T>(a: T*T): T def
    return a.a

get((C(), B())).f()
get((B(), A())).f()
get((A(), C())).f()
