
class A def
    Char c: '*'

    constructor(Char c) def
        print this.c
        this.c = c

    func toString() String def
        return this.c.toString()

class B(A) def
    Bool b

    constructor(Char c, Bool b: false) def
        this.c = c
        this.b = b

    func toString() String def
        return "{this.c} {this.b}"

class C(B) def
    Int x

class D(A) def
    Int x: 42

    constructor() def
        this.c = '?'

    func toString() String def
        return "{this.c} {this.x}"


func f(A a) def
    print a.toString()

f(A('A'))
f(B('B', b=true))
f(C(c='C'))
f(D())
