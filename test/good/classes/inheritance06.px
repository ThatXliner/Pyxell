
class A def
    Char c: '*'

    constructor(Char c) def
        print self.c
        self.c = c

    func toString() String def
        return self.c.toString()

class B(A) def
    Bool b

    constructor(Char c, Bool b: false) def
        self.c = c
        self.b = b

    func toString() String def
        return "{self.c} {self.b}"

class C(B) def
    Int x

class D(A) def
    Int x: 42

    constructor() def
        self.c = '?'

    func toString() String def
        return "{self.c} {self.x}"


func f(A a) def
    print a.toString()

f(A('A'))
f(B('B', b=true))
f(C(c='C'))
f(D())
